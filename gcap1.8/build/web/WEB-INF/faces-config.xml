<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/connexion.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>agent</from-outcome>
            <to-view-id>/secure/accueil_agent.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>invalide</from-outcome>
            <to-view-id>/connexion.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/secure/accueil_manager.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>CSouDD</from-outcome>
            <to-view-id>/secure/accueil_manager_dd.xhtml</to-view-id>
        </navigation-case> 
        <navigation-case>
            <from-outcome>DG</from-outcome>
            <to-view-id>/secure/accueil_manager_dg.xhtml</to-view-id>
        </navigation-case>     
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secure/accueil_agent.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>valide</from-outcome>
            <to-view-id>/secure/demande_cong_an.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/secure/*</from-view-id>
        <navigation-case>
            <from-outcome>reponse_traitement</from-outcome>
            <to-view-id>/secure/notif_agent.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/secure/notif_agent.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>reponse_traitement</from-outcome>
            <to-view-id>/secure/notif_agent.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/secure/demande_cong_an.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/secure/demande_cong_an.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secure/accueil_manager.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>traiterdem</from-outcome>
            <to-view-id>/secure/traiter_dem_abs.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secure/accueil_manager_dd.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>traiterdem</from-outcome>
            <to-view-id>/secure/traiter_dem_abs.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secure/accueil_manager_dg.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>traiterdem</from-outcome>
            <to-view-id>/secure/traiter_dem_abs.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>connexionManagedBean</managed-bean-name>
        <managed-bean-class>managedbean.ConnexionManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>demCongManagedBean</managed-bean-name>
        <managed-bean-class>managedbean.DemCongManagedBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>traiterDemCongAn</managed-bean-name>
        <managed-bean-class>managedbean.TraiterDemCongAnBean</managed-bean-class>
    
        <managed-bean-scope>view</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>nbDemAtraiteManagedBean</managed-bean-name>
        <managed-bean-class>managedbean.NbDemAtraiteManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>reponseTraitementManagedBean</managed-bean-name>
        <managed-bean-class>managedbean.ReponseTraitementManagedBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>testDemandeCongeBean</managed-bean-name>
        <managed-bean-class>managedbean.testDemandeCongeBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
</faces-config>
